{
  "name": "express-fileupload",
  "version": "1.2.1",
  "author": "Richard Girges <richardgirges@gmail.com>",
  "description": "Simple express file upload middleware that wraps around Busboy",
  "main": "./lib/index",
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec",
    "jest": "jest --watch",
    "lint": "eslint ./",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "busboy": "^0.3.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "express",
    "file-upload",
    "upload",
    "forms",
    "multipart",
    "files",
    "busboy",
    "middleware"
  ],
  "license": "MIT",
  "repository": "richardgirges/express-fileupload",
  "jest": {
    "testMatch": [
      "**/?(*.)+(test).[jt]s?(x)"
    ]
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "coveralls": "^3.0.14",
    "eslint": "^7.5.0",
    "express": "^4.17.1",
    "istanbul": "^0.4.5",
    "jest": "^27.0.6",
    "md5": "^2.2.1",
    "mocha": "^8.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2"
  }
}
